CREATE TABLE [dbo].[People] (
    [PeopleId]    INT           IDENTITY (1, 1) NOT NULL,
    [Name]        NVARCHAR (50) NOT NULL,
    [Email]       NVARCHAR (50) NOT NULL,
    [Phone]       NVARCHAR (15) NOT NULL,
    [TableNumber] INT           NOT NULL,
    PRIMARY KEY CLUSTERED ([PeopleId] ASC)
);

